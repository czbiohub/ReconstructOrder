    RowBox[{"{", 
     RowBox[{"0", ",", "0", ",", 
      RowBox[{"Cos", "[", 
       RowBox[{"\[Pi]", "/", "2"}], "]"}], ",", 
      RowBox[{"Sin", "[", 
       RowBox[{"\[Pi]", "/", "2"}], "]"}]}], "}"}], ",", 
